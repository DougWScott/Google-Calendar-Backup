<h1>Editing XML feed</h1>

<% form_for(@grablist) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :feedurl %><br />
    <%= f.text_field :feedurl %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @grablist %> |
<%= link_to 'Back', grablists_path %>